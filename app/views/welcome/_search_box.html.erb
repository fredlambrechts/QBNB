
  <div class="container">
  <div class="row center">
    <div class="col-xs-12">
      <div>
        <%= form_tag '/rentals', method: :get, :class => "form-horizontal", :role => 'form' do %>
          <div class="form-group">
            <div class="col-xs-3"></div>
              <div class="col-xs-6">
                <%= text_field_tag :adress_city, nil, class: "form-control", placeholder: 'Where do you want to go?' %>
              </div>
              <div class="col-xs-3"></div>
            </div>
            <div class="form-group">
              <div class="col-xs-3"></div>
              <div class="col-xs-6">
                <%= submit_tag 'Search', class: 'btn btn-success' %>
              </div>
              <div class="col-xs-3"></div>
            </div>
        <% end %>
    </div>
  </div>
</div>
</div>